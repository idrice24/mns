{
  "name": "mns",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start:fr": "ng serve  --configuration=fr --open --allowedHosts --poll",
    "start": "ng serve    --open --allowedHosts --poll",
    "startv": "ng serve --verbose --open --allowedHosts --poll",
    "build": "ng build",
    "test2": "npm run test --  --no-watch --no-progress",
    "test": "ng test",
    "clean": "rimraf ./dist",
    "test-deploy": "npm run test --  --no-watch --no-progress",
    "lint": "ng lint",
    "lint:fix": "ng lint   --fix",
    "e2e": "ng e2e",
    "e2e-deploy": "ng e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "deploy": "ng lint && ng build --prod --output-path docs --base-href /mns/ && git status",
    "deploy-test": "ng lint && ng build --prod --output-path docs --base-href /mns/ && git status && git config --global user.email 'gzeleu@gmx.de' &&  git config --global user.name 'Ghislain' && git add * -f && git commit -m 'UPDATED GITHUB DOCS FOLDER' && git push ",
    "compodoc": "npx compodoc -p tsconfig.json -o -s",
    "dev": "npm run deploy && ng serve --open",
    "fire": "ng lint && ng build --prod --output-path public --verbose ",
    "fire-deploy": "firebase login && npm run fire && firebase deploy",
    "build:ci": "npm run clean && npm run test-deploy && npm run build:prod --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "11.0.5",
    "@angular/localize": "^11.2.14",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@compodoc/compodoc": "^1.1.16",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-in-memory-web-api": "^0.11.0",
    "aos": "^2.3.4",
    "bootstrap": "^4.6.1",
    "chart.js": "^3.6.2",
    "emailjs-com": "^2.6.4",
    "firebase": "^8.10.0",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "ngx": "^2.4.0",
    "ngx-moment": "^5.0.0",
    "popper.js": "^1.16.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
