    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "^3.10.2",
