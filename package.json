{
  "name": "mns",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --verbose  --open",
    "build": "ng build",
    "test2": "ng test ",
    "test": "ng test",
    "test-deploy": "npm run test --  --no-watch --no-progress",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-deploy": "ng e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "deploy": "ng lint && ng build --prod --output-path docs --base-href /mns/ ",
    "compodoc": "npx compodoc -p tsconfig.json -o -s",
    "dev": "npm run deploy && ng serve --open",
    "fire": "ng lint && ng build --prod --output-path public --verbose ",
    "fire-deploy": "npm run fire && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.1",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "10.1.1",
    "@angular/compiler": "10.1.1",
    "@angular/core": "10.1.1",
    "@angular/fire": "^6.0.4",
    "@angular/forms": "10.1.1",
    "@angular/platform-browser": "10.1.1",
    "@angular/platform-browser-dynamic": "10.1.1",
    "@angular/router": "10.1.1",
    "@compodoc/compodoc": "^1.1.11",
    "@types/googlemaps": "^3.40.2",
    "angular-in-memory-web-api": "^0.11.0",
    "aos": "^2.3.4",
    "bootstrap": "^4.5.2",
    "firebase": "^8.1.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.3",
    "rxjs": "^6.6.3",
    "simple-line-icons": "^2.5.5",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.3"
  },
