    "@angular/platform-browser-dynamic": "13.1.1",
    "@angular/router": "13.1.1",
    "@compodoc/compodoc": "^1.1.16",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/googlemaps": "^3.40.3",
    "angular-in-memory-web-api": "^0.13.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.0",
    "emailjs-com": "^3.2.0",
    "firebase": "^9.6.1",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "ngx": "^2.3.0",
    "ngx-moment": "^6.0.2",
